{
  "name": "nfd-discord",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "-- -- -- -- START SCRIPTS -- -- -- -- ": "--",
    "start": "NODE_ENV=local npx ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "start:local": "NODE_ENV=local npm run knex:reset:local && NODE_ENV=local npx ts-node-dev --inspect --respawn --transpileOnly ./src/index.ts",
    "start:dev": "NODE_ENV=development npm run knex:reset:dev && npx ts-node src/index.ts",
    "start:prod": "NODE_ENV=production npm run knex:migrate:prod && npx ts-node src/index.ts",
    "-- -- -- -- DATABASE SCRIPTS -- -- -- -- ": "--",
    "knex:reset:dev": "NODE_ENV=development npm run knex:rollback:dev && npm run knex:migrate:dev && npx knex --knexfile ./database/knexfile.js seed:run",
    "knex:rollback:dev": "NODE_ENV=development npx knex --knexfile ./database/knexfile.js migrate:rollback --all",
    "knex:migrate:dev": "NODE_ENV=development npx knex --knexfile ./database/knexfile.js migrate:latest",
    "knex:reset:local": "NODE_ENV=local npm run knex:rollback:local && npm run knex:migrate:local && npx knex --knexfile ./database/knexfile.js seed:run",
    "knex:rollback:local": "NODE_ENV=local npx knex --knexfile ./database/knexfile.js migrate:rollback --all",
    "knex:migrate:local": "NODE_ENV=local npx knex --knexfile ./database/knexfile.js migrate:latest",
    "knex:reset:prod": "NODE_ENV=production npm run knex:rollback:dev && npm run knex:migrate:dev && npx knex --knexfile ./database/knexfile.js seed:run",
    "knex:rollback:prod": "NODE_ENV=production npx knex --knexfile ./database/knexfile.js migrate:rollback --all",
    "knex:migrate:prod": "NODE_ENV=production npx knex --knexfile ./database/knexfile.js migrate:latest",
    "-- -- -- -- DEPLYOMENT SCRIPTS -- -- -- -- ": "--",
    "deploy": "sh ./deployment/scripts/deploy.sh",
    "-- -- -- -- DOCKER SCRIPTS -- -- -- -- ": "--",
    "docker:dev": "npm run docker:build:dev && npm run docker:up:dev",
    "docker:prod": "npm run docker:build:prod && npm run docker:up:prod",
    "docker:up:dev": "docker-compose -f ./deployment/docker/dev/docker-compose.yml up",
    "docker:up:prod": "docker-compose -f ./deployment/docker/prod/docker-compose.yml up",
    "docker:build:dev": "docker-compose -f ./deployment/docker/dev/docker-compose.yml build --no-cache",
    "docker:build:prod": "docker-compose -f ./deployment/docker/dev/docker-compose.yml build --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dottjt/nfd-discord.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dottjt/nfd-discord/issues"
  },
  "homepage": "https://github.com/dottjt/nfd-discord#readme",
  "dependencies": {
    "discord.js": "^12.2.0",
    "dotenv": "^7.0.0",
    "dotenv-flow": "^3.1.0",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.1",
    "graphql-yoga": "^1.18.3",
    "knex": "^0.21.1",
    "koa": "^2.12.1",
    "koa-router": "^9.0.1",
    "moment": "^2.29.4",
    "node-cron": "^2.0.3",
    "node-emoji": "^1.10.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "require-graphql-file": "^1.0.1",
    "snoostorm": "^1.5.0",
    "snoowrap": "^1.21.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "twitter-lite": "^0.14.0",
    "typescript": "^3.9.5",
    "uuid": "^8.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0"
  }
}
